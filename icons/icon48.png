</span><span><span class="hljs-keyword">const</span></span><span> mongoose = </span><span><span class="hljs-built_in">require</span></span><span>(</span><span><span class="hljs-string">'mongoose'</span></span><span>);
</span><span><span class="hljs-keyword">const</span></span><span> cors = </span><span><span class="hljs-built_in">require</span></span><span>(</span><span><span class="hljs-string">'cors'</span></span><span>);
</span><span><span class="hljs-built_in">require</span></span><span>(</span><span><span class="hljs-string">'dotenv'</span></span><span>).</span><span><span class="hljs-title function_">config</span></span><span>();

</span><span><span class="hljs-keyword">const</span></span><span> app = </span><span><span class="hljs-title function_">express</span></span><span>();
app.</span><span><span class="hljs-title function_">use</span></span><span>(</span><span><span class="hljs-title function_">cors</span></span><span>());
app.</span><span><span class="hljs-title function_">use</span></span><span>(express.</span><span><span class="hljs-title function_">json</span></span><span>());

mongoose.</span><span><span class="hljs-title function_">connect</span></span><span>(process.</span><span><span class="hljs-property">env</span></span><span>.</span><span><span class="hljs-property">MONGO_URI</span></span><span>, { </span><span><span class="hljs-attr">useNewUrlParser</span></span><span>: </span><span><span class="hljs-literal">true</span></span><span> });

</span><span><span class="hljs-keyword">const</span></span><span> </span><span><span class="hljs-title class_">Track</span></span><span> = mongoose.</span><span><span class="hljs-title function_">model</span></span><span>(</span><span><span class="hljs-string">'Track'</span></span><span>, {
  </span><span><span class="hljs-attr">site</span></span><span>: </span><span><span class="hljs-title class_">String</span></span><span>,
  </span><span><span class="hljs-attr">duration</span></span><span>: </span><span><span class="hljs-title class_">Number</span></span><span>,
  </span><span><span class="hljs-attr">timestamp</span></span><span>: { </span><span><span class="hljs-attr">type</span></span><span>: </span><span><span class="hljs-title class_">Date</span></span><span>, </span><span><span class="hljs-attr">default</span></span><span>: </span><span><span class="hljs-title class_">Date</span></span><span>.</span><span><span class="hljs-property">now</span></span><span> },
});

app.</span><span><span class="hljs-title function_">post</span></span><span>(</span><span><span class="hljs-string">'/api/track'</span></span><span>, </span><span><span class="hljs-keyword">async</span></span><span> (req, res) =&gt; {
  </span><span><span class="hljs-keyword">const</span></span><span> { site, duration } = req.</span><span><span class="hljs-property">body</span></span><span>;
  </span><span><span class="hljs-keyword">await</span></span><span> </span><span><span class="hljs-title class_">Track</span></span><span>.</span><span><span class="hljs-title function_">create</span></span><span>({ site, duration });
  res.</span><span><span class="hljs-title function_">sendStatus</span></span><span>(</span><span><span class="hljs-number">200</span></span><span>);
});

app.</span><span><span class="hljs-title function_">get</span></span><span>(</span><span><span class="hljs-string">'/api/report/weekly'</span></span><span>, </span><span><span class="hljs-keyword">async</span></span><span> (req, res) =&gt; {
  </span><span><span class="hljs-keyword">const</span></span><span> oneWeekAgo = </span><span><span class="hljs-keyword">new</span></span><span> </span><span><span class="hljs-title class_">Date</span></span><span>(</span><span><span class="hljs-title class_">Date</span></span><span>.</span><span><span class="hljs-title function_">now</span></span><span>() - </span><span><span class="hljs-number">7</span></span><span> * </span><span><span class="hljs-number">86400000</span></span><span>);
  </span><span><span class="hljs-keyword">const</span></span><span> data = </span><span><span class="hljs-keyword">await</span></span><span> </span><span><span class="hljs-title class_">Track</span></span><span>.</span><span><span class="hljs-title function_">aggregate</span></span><span>([
    { </span><span><span class="hljs-attr">$match</span></span><span>: { </span><span><span class="hljs-attr">timestamp</span></span><span>: { </span><span><span class="hljs-attr">$gte</span></span><span>: oneWeekAgo } } },
    { </span><span><span class="hljs-attr">$group</span></span><span>: { </span><span><span class="hljs-attr">_id</span></span><span>: </span><span><span class="hljs-string">"$site"</span></span><span>, </span><span><span class="hljs-attr">total</span></span><span>: { </span><span><span class="hljs-attr">$sum</span></span><span>: </span><span><span class="hljs-string">"$duration"</span></span><span> } } },
    { </span><span><span class="hljs-attr">$sort</span></span><span>: { </span><span><span class="hljs-attr">total</span></span><span>: -</span><span><span class="hljs-number">1</span></span><span> } }
  ]);
  res.</span><span><span class="hljs-title function_">json</span></span><span>(data);
});

app.</span><span><span class="hljs-title function_">listen</span></span><span>(</span><span><span class="hljs-number">3000</span></span><span>, </span><span><span class="hljs-function">() =&gt;</span></span><span> </span><span><span class="hljs-variable language_">console</span></span><span>.</span><span><span class="hljs-title function_">log</span></span><span>(</span><span><span class="hljs-string">"Server running on port 3000"</span></span><span>));
</span></span></code></div></div></pre>
